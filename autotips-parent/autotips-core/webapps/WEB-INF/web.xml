<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <servlet>
    <servlet-name>AutoTipsServlet</servlet-name>
    <servlet-class>org.leopard.autotips.web.AutoTipsServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>AutoTipsConsoleServlet</servlet-name>
    <servlet-class>org.leopard.autotips.web.console.AutoTipsConsoleServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AutoTipsServlet</servlet-name>
    <url-pattern>/AutoTipsServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AutoTipsConsoleServlet</servlet-name>
    <url-pattern>/AutoTipsConsoleServlet</url-pattern>
  </servlet-mapping>
  <listener>
    <listener-class>org.leopard.autotips.web.AutoTipsStartupListener</listener-class>
  </listener>
</web-app>